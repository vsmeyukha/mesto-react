новую карточку\n        const newCards = cards.map((c) => c._id === card._id ? newCard : c);\n      // Обновляем стейт\n        setCards(newCards);\n      });\n  }\n\n  function handleCardDelete(card) {\n    api.deleteCard(card._id)\n      .then((cardForDeleting) => {\n        const cardsWithoutACard = cards.filter((c) => {\n          if (c._id !== card._id) {\n            return c;\n          }\n        });\n        setCards(cardsWithoutACard);\n      })\n  }\n\n  return (\n    <section className=\"cards\">\n      {cards.map(card => {\n        return (\n          <Card\n            card={card}\n            key={card._id}\n            onCardClick={props.onCardClick}\n            onLikeClick={() => {\n              handleCardLike(card)\n            }}\n            onCardDelete={() => {\n              handleCardDelete(card)\n            }}\n          />\n        );\n      })}\n    </section>\n  );\n}\n\nexport default Cards;","/Users/victorsmeyukha/Desktop/dev/mesto-react-attempt2/src/components/Profile.js",[],"/Users/victorsmeyukha/Desktop/dev/mesto-react-attempt2/src/components/Card.js",[],"/Users/victorsmeyukha/Desktop/dev/mesto-react-attempt2/src/utils/Api.js",[],"/Users/victorsmeyukha/Desktop/dev/mesto-react-attempt2/src/contexts/CurrentUserContext.js",[],"/Users/victorsmeyukha/Desktop/dev/mesto-react-attempt2/src/components/EditProfilePopup.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":41,"column":52,"nodeType":"69","messageId":"70","endLine":41,"endColumn":54},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"array-callback-return","Array.prototype.filter() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","no-global-assign","no-unsafe-negation"]